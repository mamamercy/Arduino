// Fallen Kingdom LED Light Show
const int W = 13;
const int R = 12;
const int G = 11;
const int B = 10;
const int R2 = 9; 
const int G2 = 6;
const int B2 = 5;
const int W2 = 4;
void setup() {
  pinMode(W, OUTPUT);
  pinMode(R, OUTPUT);
  pinMode(G, OUTPUT);
  pinMode(B, OUTPUT);
  pinMode(R2, OUTPUT);
  pinMode(G2, OUTPUT);
  pinMode(B2, OUTPUT);
  pinMode(W2, OUTPUT);
}

void loop() {
  // Sequence 1
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
 
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW); 
  delay(250);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  Flash();
  // End of Sequence 1
  
  // Sequence 2
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
 
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW); 
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(R, LOW);
  delay(420);
  digitalWrite(G2, LOW);
  digitalWrite(G, LOW);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  Flash();
  // End of Sequence 2
  
  // Sequence 3
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(420);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  Flash();
  // End of Sequence 3

  // Corus
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(500);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(250);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(500);
  Opposite();
  Opposite();
  delay(200);
  digitalWrite(W2, HIGH);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(B2, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(W, HIGH);
  Flash2();
  Flash2();
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  delay(250);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  Reset();
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(B, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(420);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(250);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(250);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(250);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(250);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(250);
  digitalWrite(R2, HIGH);
  digitalWrite(B, HIGH);
  delay(250);
  digitalWrite(G2, HIGH);
  digitalWrite(G, HIGH);
  delay(250);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(B, LOW);
  digitalWrite(G2, LOW);
  digitalWrite(G, LOW);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(B, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(B, LOW);
  digitalWrite(G2, LOW);
  digitalWrite(G, LOW);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(B, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(B, LOW);
  delay(250);
  digitalWrite(G2, LOW);
  digitalWrite(G, LOW);
  delay(250);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(1000);
  // End of Corus
  
  // Sequence 1
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
 
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  delay(420);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW); 
  delay(250);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  delay(420);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  Flash();
  // End of Sequence 1
  
  // Sequence 2
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW);
  delay(420);
  digitalWrite(R, LOW);
  digitalWrite(B2, LOW);
  delay(420);
  digitalWrite(G, LOW);
  digitalWrite(G2, LOW);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  digitalWrite(W, HIGH);
  digitalWrite(W2, HIGH);
 
  delay(420);
  digitalWrite(R, HIGH);
  digitalWrite(B2, HIGH);
  delay(420);
  digitalWrite(G, HIGH);
  digitalWrite(G2, HIGH);
  delay(420);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(W2, LOW); 
  delay(250);
  digitalWrite(B2, LOW);
  digitalWrite(R, LOW);
  delay(420);
  digitalWrite(G2, LOW);
  digitalWrite(G, LOW);
  delay(250);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  delay(420);
  Flash();
  // End of Sequence 2
  delay(10000);
  
  
  
  
}
void Flash() {
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(420);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(420);
}
  void Flash2() {
  digitalWrite(W, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(B, HIGH);
  digitalWrite(R2, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(B2, HIGH);
  digitalWrite(W2, HIGH);
  delay(250);
  digitalWrite(W, LOW);
  digitalWrite(R, LOW);
  digitalWrite(G, LOW);
  digitalWrite(B, LOW);
  digitalWrite(R2, LOW);
  digitalWrite(G2, LOW);
  digitalWrite(B2, LOW);
  digitalWrite(W2, LOW);
  delay(250);
}
  void Opposite() {
  digitalWrite(W2, LOW);
  digitalWrite(G2, LOW);
  digitalWrite(R, LOW);
  digitalWrite(B, LOW);
  digitalWrite(W, HIGH);
  digitalWrite(G, HIGH);
  digitalWrite(R2, HIGH);
  digitalWrite(B2, HIGH);
  delay(200);
  digitalWrite(W, LOW);
  digitalWrite(G, LOW);
  digitalWrite(R2, LOW);
  digitalWrite(B2, LOW);
  digitalWrite(W2, HIGH);
  digitalWrite(G2, HIGH);
  digitalWrite(R, HIGH);
  digitalWrite(B, HIGH);
  delay(200); 
 }
  void Reset() {
    digitalWrite(W, LOW);
    digitalWrite(R, LOW); 
    digitalWrite(G, LOW);
    digitalWrite(B, LOW);
    digitalWrite(R2, LOW);
    digitalWrite(G2, LOW);
    digitalWrite(B2, LOW);
    digitalWrite(W2, LOW);
  }



